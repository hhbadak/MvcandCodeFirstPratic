@model IEnumerable<MvcandCodeFirstPratic.Models.Suppliers>
@using MvcandCodeFirstPratic.Models
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<div class="container table">
    <table class="table table-hover">
        <thead>
            <tr>
                <th>ID</th>
                <th>Şirket Adı</th>
                <th>Telefon Numarası</th>
                <th>Durum</th>
            </tr>
        </thead>
        <tbody>
            @foreach (Suppliers item in Model)
            {
                <tr>
                    <td>@item.ID</td>
                    <td>@item.CompanyName</td>
                    <td>@item.Phone</td>
                    <td>@item.status</td>
                </tr>
            }
        </tbody>
    </table>
</div>
